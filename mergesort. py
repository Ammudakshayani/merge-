for n in range(size):
 numbers = int(input("enter any number:\t"))
 arr.append(numbers)
arr_size = len(arr)
print(â€œ The sorted list is:â€)
mergeSort(arr, 0, arr_size - 1)
printArray(arr, arr_size)
